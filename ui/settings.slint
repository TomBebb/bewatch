@rust-attr( derive(bincode::Encode, bincode::Decode))
export struct PlaybackSettings  {
    subtitleIndex: int, audioIndex: int, autoPlayNext: bool}

@rust-attr( derive(bincode::Encode, bincode::Decode))
export struct MySettings {
    playback: PlaybackSettings}

export global Settings {

    in-out property <MySettings> value;
    callback change(MySettings);
}
